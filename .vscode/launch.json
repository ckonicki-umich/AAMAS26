    {
      "name": "Python Debugger: Run PBE Algorithm Experiment",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/PBE experiments/pbe_algorithm_experiment.py",
      "console": "integratedTerminal",
      "args": [
          "0",
          "0",
          "0"
      ]
    },
    {
      "name": "Python Debugger: Generate GenGoof Games",
      "type": "debugpy",
